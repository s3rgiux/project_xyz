<?xml version="1.0"?>
<launch>
    <node name="uvc_camera_node" pkg="uvc_camera" type="uvc_camera_node" output="screen">
        <param name="width" value="1280"/>
        <param name="height" value="720"/>
    </node>
    <node pkg="camera_calibration" type="cameracalibrator.py" name="cameracalibrator">
        <param name="size" value="8x6" />
        <param name="square" value="0.025" />
        <remap from="image" to="image_raw" />
    </node>
</launch>